<div class="header-container header-container-public" role="navigation">
  <div data-react-class="T.HeaderContainer" data-react-props="{"user":{"profile_image_url":"https://qiita-image-store.s3.amazonaws.com/0/136530/profile-images/1473719981","team_only":false,"teams":[],"url_name":"marshmallow0319"},"team":null,"news":{"type":"News","content":"ストックの他に「いいね」が追加されました","url":"http://blog.qiita.com/post/153200849029/qiita-like-button"},"initial_unread_count":0,"siteid_image":"http://cdn.qiita.com/siteid-reverse.png","is_team_page":false,"on_team_setting":false,"show_post_menu":true,"show_search_menu":true,"is_fluid":false,"locale":"ja"}">
    <header data-reactroot class="shared-header shared-header-public">
      <div class="shared-header_inner">
        <div class="shared-header_left">
          <span class="shared-header_logo open">
            <%= link_to root_path, class: "shared-header_logo_main" do%>
              <span class="shared-header_logo_figure">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 75 26">
                  <title>Qiita</title>
                  <path d="M24.6 12.2C24.6 6.2 20 0 12.2 0 6.2 0 0 4.6 0 12.4c0 6 4.6 12.2 12.4 12.2 2.5 0 5-.8 7.1-2.3l3.6 3.6 1.6-1.6-3.4-3.4c2.1-2.3 3.3-5.3 3.3-8.7zm-12.4-10c5.7 0 10.2 4.4 10.2 10.1 0 4.9-3.8 10.1-10.1 10.1s-10.1-5.1-10.1-10c0-6.4 5.1-10.2 10-10.2zM30.5 7.1h2.1v17h-2.1zM31.5 4.8c-1 0-1.8-.8-1.8-1.8s.8-1.8 1.8-1.8 1.8.8 1.8 1.8-.8 1.8-1.8 1.8zM40 4.8c-1 0-1.8-.8-1.8-1.8S39 1.2 40 1.2s1.8.8 1.8 1.8S41 4.8 40 4.8zM39 7.1h2.1v17H39zM53.7 24.4c-3.9 0-6.2-3-6.2-6v-16h2.1v4.7H57v2.1h-7.4v9.3c0 1.9 1.5 3.9 4.1 3.9.7 0 1.4-.2 2-.6l.2-.1 1 1.8-.2.1c-.9.5-2 .8-3 .8zM67.6 24.5c-5.7 0-9.1-4.5-9.1-8.9 0-5.7 4.5-9.1 8.9-9.1 2.3 0 4.1.8 5.4 2.3V7.1h2.1v17h-2.1v-1.9c-1.2 1.5-3 2.3-5.2 2.3zm-.2-15.9c-3.4 0-6.8 2.6-6.8 7 0 3.4 2.6 6.8 7 6.8 2 0 4-.9 5.3-2.5v-8.8c-1.4-1.6-3.3-2.5-5.5-2.5z"></path>
                </svg>
              </span>
            <% end %>
          </span>
          <form class="navbar-form navbar-left search-form" action-"/search" method="get">
            <div class="search-form_query-wrapper">
              <input class="form-control search-form_query" placeholder="キーワードを入力" autocomplete="off" name="q" value style="width: 220px;">
              <div class="search-form_wise-search-button" style="visibility: visible;">
                <i class="fa fa-caret-down"></i>
              </div>
            </div>
          </form>
          <div class="navbar-text header-news hidden-xs hidden-sm">
            <div class="header-news_label label label-warning">News</div>
            <a class="navbar-link" target="_blank" href="#">
              ストックの他に「いいね」が追加されました
            </a>
          </div>
        </div>
        <div class="shared-header_right">
          <% if user_signed_in? %>
            <div class="shared-header_search-icon">
              <a data-tiggle="model" data-target="#js-search-model">
                <i class="fa fa-search"></i>
              </a>
            </div>
            <div class="shared-header_search-model in modal fade" aria-hidden="false" id="js-search-model">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-body">
                    <form action="/search" method="get">
                      <input type="hidden" name="sort" id="sort" value="rel">
                      <div class="row">
                        <div class="col-xs-9">
                          <input type="text" name="q" id="q" class="form-control">
                        </div>
                        <div class="col-xs-3">
                          <input type="submit" name="commit" value="Search" class="btn btn-default btn-sm">
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
            <div class="shared-header_new-item btn-group">
              <%= link_to new_article_path, class: "shared-header_new-item_btn shared-header_new-item_post btn" do %>
                <i class="fa fa-lg fa-edit"></i>
                <!-- react-text: 36 -->
                投稿する
                <!-- /react-text -->
              <% end %>
              <a class="shared-header_new-item_btn dropdown-toggle btn">
                <i class="fa fa-caret-down"></i>
                <span class="shared-header_new-item_xs-edit">
                  <i class="fa fa-edit"></i>
                </span>
              </a>
            </div>
            <div class="shared-header_stock-items">
              <%= link_to "ストック一覧", stocks_path %>
            </div>
            <div class="shared-header_stock-items_xs-stock">
              <a href="#" title="ストック一覧">
                <i class="fa fa-folder"></i>
              </a>
            </div>
            <div class="shared-header_notifications">
              <div class="global-notifications open active">
                <div class="global-notifications__surface">
                  <div class="global-notifications_count">
                    0
                  </div>
                </div>
              </div>
            </div>
            <div class="shared-header_user">
              <a class="shared-header_user_clickable" data-toggle="dropdown" href="#" aria-expanded="false">
                <%= image_tag current_user.avatar, class: "shared-header_user_image", size: "32x32" %>
              </a>
              <ul class="dropdown-menu shared-header_user_dropdown">
                <li>
                  <%= link_to user_path(current_user) do%>
                    <i class="fa fa-fw fa-user"></i>
                    <!-- react-text: 57 -->
                    マイページ
                    <!-- /react-text -->
                  <% end %>
                </li>
                <li>
                  <a href="#">
                    <i class="fa fa-fw fa-cog"></i>
                    <!-- react-text: 61 -->
                    設定
                    <!-- /react-text -->
                  </a>
                </li>
                <li>
                  <a href="#">
                    <i class="fa fa-fw fa-question"></i>
                    <!-- react-text: 65 -->
                    ヘルプ
                    <!-- /react-text -->
                  </a>
                  <%= link_to destroy_user_session_path, method: :delete do %>
                    <i class="fa fa-fw fa-sign-out"></i>
                    ログアウト
                  <% end %>
                </li>
              </ul>
            </div>
          </div>
        <% else %>
          <div class="shared-header_register-user">
            <%= link_to "ユーザ登録", new_user_registration_path, class: "btn btn-warning navbar-btn navbar-right" %>
          </div>
          <div class="shared-header_login-user">
            <%= link_to "ログイン", new_user_session_path %>
          </div>
        <% end %>
      </div>
    </header>
  </div>
</div>
